{
    "productName": "Denox",
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "identifier": "no.kvihaugen.denox",
    "build": {
        "frontendDist": "../out",
        "devUrl": "http://localhost:3000",
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build"
    },
    "app": {
        "windows": [
            {
                "title": "Denox",
                "width": 900,
                "height": 600,
                "minWidth": 900,
                "minHeight": 600,
                "resizable": true,
                "fullscreen": false,
                "decorations": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/logo.svg",
            "icons/logo.png",
            "icons/logo.ico"
        ]
    }
}